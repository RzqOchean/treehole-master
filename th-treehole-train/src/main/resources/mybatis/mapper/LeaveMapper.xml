<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.treehole.train.dao.LeaveMapper">

    <!--请假信息统计-->
    <select id="findByLeavePeopleIdAndLeavePeopleType"
            resultType="com.treehole.framework.domain.train.Leave"
            parameterType="java.util.HashMap">
        SELECT *
        FROM leaves
        WHERE leave_people_type=#{peopleType}
        AND leave_people_id = #{peopleId}

    </select>

    
</mapper>